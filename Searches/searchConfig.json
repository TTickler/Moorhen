{

    "query": {

        "filtered": {

            "query" : {

                "multi_match": {

                    "query": "dockertest",

                    "fields": ["client"]

                }

            },

            "filter": {

                "bool": {

                    "must": {

                        "range" : { "CPU.Temperature": { "gte": 20 } }

                    },

                    "must_not": {

                        "range" : { "CPU.Temperature": { "lte": 10 } }

                    },

                    "should": {

                        "term": { "client": "dockertest" }

                    }

                }

            }

        }

    },

    "_source" : ["client","CPU.Temperature"]

}
